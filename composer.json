{
  "name": "in2code/fetchurl",
  "description": "Fetch an url and show the content in a TYPO3 Frontend.",
  "keywords": ["typo3", "fetch", "iframe"],
  "homepage": "https://github.com/einpraegsam/fetchurl",
  "authors": [
    {
      "name": "Alex Kellner",
      "email": "alexander.kellner@in2code.de"
    }
  ],
  "type": "typo3-cms-extension",
  "license": "GPL-3.0",
  "require": {
    "typo3/cms-core": ">=7.6.0 <9.9.99"
  },
  "replace": {
    "fetchurl": "self.version"
  },
  "autoload": {
    "psr-4": {
      "In2code\\Fetchurl\\": "Classes"
    }
  }
}
